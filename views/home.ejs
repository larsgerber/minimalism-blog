<%- include('partials/head'); -%>  
<%- include('partials/header'); -%>

<ul class="post-list">
    <li class="post-item">
        <% if (data.posts.length > 0) { %> <% data.posts.forEach(post=> { %>

        <div class="meta">
            <time datetime="<%= post.created_at %>" itemprop="datePublished">
                <%= post.createdAt_local %>
            </time>
            <span>
                <a href="<%= post.link %>">
                    <%= post.title %>
                </a>
            </span>
        </div>

        <% }) %> <% } else { %>

        <h4>Im Moment sind noch keine BeitrÃ¤ge vorhanden.</h4>

        <% } %>
    </li>
</ul>

<%- include('partials/footer'); -%>
